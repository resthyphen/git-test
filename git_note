> …or create a new repository on the command line
echo "# git-test" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/resthyphen/git-test.git
git push -u origin main

> …or push an existing repository from the command line
git remote add origin https://github.com/resthyphen/git-test.git
git branch -M main
git push -u origin main
 
  //rename a branch
  566  git checkout master
  567  git branch -m master sub-branch
  568  git fetch origin
  569  git branch -u origin/sub-branch sub-branch
  570  git remote set-head origin -a


  //create a branch
    git branch test-branch
    git push origin test-branch
    git branch --set-upstream-to=origin/test-branch test-branch
